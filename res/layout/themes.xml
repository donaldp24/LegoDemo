<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
				android:id="@+id/layout_themes"
				android:orientation="vertical"
				android:layout_width="800px"
				android:layout_height="480px"
				android:background="@drawable/background"
		>
	<!-- Common Items -->
	<ImageView
			android:layout_marginLeft="290px"
			android:layout_marginTop="0px"
			android:layout_width="220px"
			android:layout_height="180px"
			android:background="@drawable/legomark"
			/>
	<Button
			android:id="@+id/btn_back"
			android:layout_marginLeft="0px"
			android:layout_marginTop="400px"
			android:layout_width="164px"
			android:layout_height="80px"
			android:background="@drawable/common_btn_back"
			/>

	<!-- Buttons of themes -->
	<Button
			android:id="@+id/btn_architecture"
			android:layout_marginLeft="18px"
			android:layout_marginTop="129px"
			android:layout_width="192px"
			android:layout_height="154px"
			android:background="@drawable/themes_btn_architecture"
			/>
	<Button
			android:id="@+id/btn_castle"
			android:layout_marginLeft="200px"
			android:layout_marginTop="129px"
			android:layout_width="192px"
			android:layout_height="154px"
			android:background="@drawable/themes_btn_castle"
			/>
	<Button
			android:id="@+id/btn_city"
			android:layout_marginLeft="418px"
			android:layout_marginTop="129px"
			android:layout_width="192px"
			android:layout_height="154px"
			android:background="@drawable/themes_btn_city"
			/>
	<Button
			android:id="@+id/btn_juniors"
			android:layout_marginLeft="600px"
			android:layout_marginTop="129px"
			android:layout_width="192px"
			android:layout_height="154px"
			android:background="@drawable/themes_btn_juniors"
			/>
	<Button
			android:id="@+id/btn_galaxy"
			android:layout_marginLeft="200px"
			android:layout_marginTop="292px"
			android:layout_width="192px"
			android:layout_height="154px"
			android:background="@drawable/themes_btn_juniors"
			/>
	<Button
			android:id="@+id/btn_starwars"
			android:layout_marginLeft="418px"
			android:layout_marginTop="292px"
			android:layout_width="192px"
			android:layout_height="154px"
			android:background="@drawable/themes_btn_starwars"
			/>
</RelativeLayout>