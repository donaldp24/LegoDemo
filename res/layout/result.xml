<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
				android:id="@+id/layout_result"
				android:orientation="vertical"
				android:layout_width="800px"
				android:layout_height="480px"
				android:background="@drawable/background"
		>
	<ImageView
			android:layout_marginLeft="290px"
			android:layout_marginTop="0px"
			android:layout_width="220px"
			android:layout_height="180px"
			android:background="@drawable/legomark"
			/>
	<ImageView
			android:id="@+id/img_resulticon"
			android:layout_marginLeft="290px"
			android:layout_marginTop="100px"
			android:layout_width="220px"
			android:layout_height="180px"
			android:background="@drawable/legomark"
			/>
	<ImageView
			android:id="@+id/img_resulttext"
			android:layout_marginLeft="490px"
			android:layout_marginTop="250px"
			android:layout_width="220px"
			android:layout_height="80px"
			android:background="@drawable/legomark"
			/>
	<Button
			android:id="@+id/btn_back"
			android:layout_marginLeft="0px"
			android:layout_marginTop="400px"
			android:layout_width="164px"
			android:layout_height="80px"
			android:background="@drawable/common_btn_back"
			/>
</RelativeLayout>